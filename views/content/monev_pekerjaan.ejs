            var id = $(this).data('termin-id');
            $("#termin-id").val(id);
            var pekerjaanId = $(this).data('termin-pekerjaan-id');
            $("#termin-pekerjaan-id").val(pekerjaanId);
            var status = $(this).data('termin-status');
            // $("#termin-status").val(status);
            var tanggalPembayaran = $(this).data('termin-tanggal-pembayaran');
            $(".termin-tanggal-pembayaran").val(tanggalPembayaran);
            var pembayaran = $(this).data('termin-pembayaran');
            $("#termin-pembayaran").val(pembayaran);
            var keterangan = $(this).data('termin-keterangan');
            $("#termin-keterangan").val(keterangan);
